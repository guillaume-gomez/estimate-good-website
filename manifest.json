{

  "description": "Display if tab contain heavy website",
  "manifest_version": 2,
  "name": "estimate-good-website",
  "version": "1.0",
  "homepage_url": "https://github.com/les-enovateurs/estimate-good-website",

  "background": {
    "scripts": ["analysis.js"]
  },

  "page_action": {
    "default_icon": "icons/bad.svg",
    "browser_style": true
  },
  
  "permissions": [
    "activeTab",
    "tabs",
    "webRequest",
    "<all_urls>",
    "webRequestBlocking"
  ]

}
